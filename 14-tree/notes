public node search(node t,int i){
       if (t==null||t.getData()==i)
       	  return t;
	else if (t.getData()<i){
	     return search(t.getRight(),i);
	else
		return search (t.getLeft(),i);
}

public node search(i){
       String retval = search(r,i);
       if (retvall==null){
       	  return "NOt FOUND";
	  }
	else 
	return ""+retval;
}

